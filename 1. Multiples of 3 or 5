#include <bits/stdc++.h> // подключаем все стандартные библиотеки
using namespace std;

int main() {
    
    int a3, a5, a15, sum;
    
    a3=(3+999)*333/2; // сумма всех чисел кратных 3 (в том числе 15)
    a5=(5+995)*199/2; // сумма всех чисел кратных 5 (в том числе 15)
    a15=(15+990)*66/2; // сумма всех чисел кратных 15
    sum=a3+a5-a15; // сумма всех чисел, кратных 3 и 5
    
    cout << sum;
    
}
